{
  "app": {
    "name": "chef-lamp",
    "site_name": "twilio-text",
    "web_dir": "/var/www",
    "mount_dir": "mount",
    "log_dir": "/var/log",
    "vhost_filename": "twilio.conf.erb",
    "mode": "dev"
  },
  "user": {
    "name": "vagrant"
  },
  "apache": {
    "mpm": "prefork"
  },
  "redisio": {
    "servers": [
      {
        "name": "master",
        "port": "6379",
        "unixsocket": "/tmp/redis.sock",
        "unixsocketperm": "755"
      }
    ]
  },
  "run_list": [
    "recipe[mariadb]",
    "recipe[apache2]",
    "recipe[redisio]",
    "recipe[redisio::enable]",
    "recipe[rabbitmq]",
    "recipe[composer]",
    "recipe[webserver]",
    "recipe[database]",
    "recipe[project]"
  ]
}